{
  "routes": [
    {
        "route": "/*"
    }
  ],
  
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [ "*.{css,scss,js,png,gif,ico,jpg,svg,hrf}" ]
  },

  "auth": {
    "identityProviders": {
      "google": {
        "registration": {
          "clientIdSettingName": "GOOGLE_CLIENT_ID",
          "clientSecretSettingName": "GOOGLE_CLIENT_SECRET"
        }
      }
    }
  }
}